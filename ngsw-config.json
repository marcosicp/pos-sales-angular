{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ],
        "urls":[
           "https://fonts.googleapis.com/**"
          ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [{
    "name": "tesisapi",
    "urls": ["https://dieteticapuchi.herokuapp.com/api/**"],
    "cacheConfig": {
      "strategy":"freshness",
      "maxSize": 20,
      "maxAge": "1h",
      "timeout": "5s" 
    }
  }]
}
